{
  "name": "arraypress/wp-register-columns",
  "version": "1.0.0",
  "description": "Easily register custom columns for any WordPress table.",
  "homepage": "https://github.com/arraypress/wp-register-columns/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\Register\\Columns\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-columns/issues",
    "source": "https://github.com/arraypress/wp-register-columns"
  },
  "keywords": [
    "wordpress",
    "pages",
    "registration",
    "templates",
    "versioning",
    "page-management",
    "backup",
    "restore",
    "wp-admin",
    "post-types",
    "meta",
    "relationships",
    "hierarchy"
  ]
}